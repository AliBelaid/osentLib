# Copy this file to .env and fill in your values before running docker compose up

# ─── PostgreSQL ───────────────────────────────────────────────────────────────
POSTGRES_DB=ausentinel
POSTGRES_USER=ausentinel
POSTGRES_PASSWORD=change_me_strong_password

# ─── RabbitMQ ─────────────────────────────────────────────────────────────────
RABBITMQ_USER=ausentinel
RABBITMQ_PASSWORD=change_me_strong_password

# ─── JWT ──────────────────────────────────────────────────────────────────────
# Must be at least 32 characters for HS256
JWT_KEY=change_me_to_a_long_random_secret_at_least_32_chars

# ─── AI Worker (optional) ─────────────────────────────────────────────────────
# LLM endpoint for the AI worker — leave blank to disable
LLM_ENDPOINT=
